   "source": [
    "# Janomeを使うための宣言 --- (*1)\n",
    "from janome.tokenizer import Tokenizer\n",
    "# Janomeの準備 --- (*2)\n",
    "tok = Tokenizer()\n",
    "# 形態素に分割 --- (*3)\n",
    "tokens = tok.tokenize(\"今日は家族でラーメンを食べに行った。\")\n",
    "# 分割結果を確認 --- (*4)\n",
    "for t in tokens:\n",
    "   print(t)"
